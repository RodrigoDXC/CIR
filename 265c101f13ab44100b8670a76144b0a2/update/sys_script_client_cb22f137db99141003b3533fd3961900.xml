        <condition/>
        <description>Calculate which age group the individual falls into based on their DOB</description>
        <field>date_of_birth</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Calculate age group from DOB</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	if (isLoading || newValue === '') {
		return;
	}

	var ga = new GlideAjax('CIRCaseManagementAjax');
	ga.addParam('sysparm_name', 'getUserAgeGroup');
	ga.addParam('sysparm_dob', newValue);
	ga.getXMLAnswer(function(answer){
		g_form.setValue('age_group', answer);
	});
}]]></script>
