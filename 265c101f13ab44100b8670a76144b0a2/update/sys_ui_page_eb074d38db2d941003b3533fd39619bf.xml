<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_page">
    <sys_ui_page action="INSERT_OR_UPDATE">
        <category>general</category>
        <client_script><![CDATA[function clickedNo(){    
	var oldValue = gel('oldValue').value;
	g_form.setValue('country', oldValue);
	GlideDialogWindow.get().destroy();
}

function clickedYes(){    
	GlideDialogWindow.get().destroy();
	g_form.save();
}]]></client_script>
        <description/>
        <direct>false</direct>
        <endpoint>x_fru_cir_cir_situation_remove_country.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
    <g:ui_form>
        <g:evaluate var="jvar_oldValue" expression="RP.getWindowProperties().oldValue" />
        <input type="hidden" id="oldValue" value="${jvar_oldValue}" />

        <g:evaluate var="jvar_newValue" expression="RP.getWindowProperties().newValue" />
        <input type="hidden" id="newValue" value="${jvar_newValue}" />

    </g:ui_form>

    <div>
        <div>
        	<h4>${gs.getMessage('cir_situation_remove_country_text')}</h4>
        </div>

        <div style="float:right; margin-right: 2px;">
            <input type="button" class="btn btn-success" onclick="clickedYes()" value="${gs.getMessage('Yes')}" />
			<input type="button" class="btn btn-danger" onclick="clickedNo()" value="${gs.getMessage('No')}" />
        </div>

    </div>
</j:jelly>]]></html>
