        <value>if(!workflow.scratchpad.interaction) {&#13;
	workflow.scratchpad.interaction = 0;&#13;
}&#13;
&#13;
var currentSteps = [];&#13;
var currentStepExec = [];&#13;
var grStep = new GlideRecord(CIRConstants.Tables.RESP_PLAN_STEP);&#13;
grStep.addQuery('active', true);&#13;
&#13;
if (workflow.scratchpad.interaction == 0) {&#13;
	grStep.addQuery('dependent_on', '');&#13;
}  else {&#13;
	var currentStep = getCurrentExecutionStep(workflow.scratchpad.current_step);&#13;
	grStep.addQuery('dependent_on','CONTAINS', workflow.scratchpad.current_step);&#13;
}&#13;
&#13;
grStep.addQuery('response_plan_template', current.response_plan_template);&#13;
grStep.query();&#13;
&#13;
while(grStep.next()) {&#13;
	if(conditionMatches(workflow.scratchpad.current_step, grStep)) {&#13;
		currentStepExec.push(createExecutionStep(grStep));&#13;
		currentSteps.push(grStep.sys_id.toString());&#13;
	} else {&#13;
		workflow.scratchpad.end_workflow = true;&#13;
	}&#13;
}&#13;
&#13;
workflow.scratchpad.current_step = currentSteps.toString();&#13;
workflow.scratchpad.interaction++;&#13;
current.current_step = currentStepExec.toString();&#13;
&#13;
function conditionMatches(lastStep, grCurStep) {&#13;
	var answer = true;&#13;
	&#13;
	if(lastStep) {&#13;
		var grExcStep = new GlideRecord(CIRConstants.Tables.RESP_PLAN_EXEC_STEP);&#13;
		grExcStep.addQuery('response_plan_step', 'IN', lastStep);&#13;
		grExcStep.addQuery('response_plan', current.sys_id.toString());&#13;
		grExcStep.addEncodedQuery(grCurStep.condition.toString());&#13;
		grExcStep.query();&#13;
		&#13;
		answer = grExcStep.hasNext();&#13;
	}&#13;
	return answer;&#13;
}&#13;
&#13;
function getCurrentExecutionStep(sys_id){&#13;
	var gr = new GlideRecord(CIRConstants.Tables.RESP_PLAN_EXEC_STEP);&#13;
	if(gr.get(sys_id.toString())) {&#13;
		return gr;&#13;
	}&#13;
}&#13;
&#13;
function createExecutionStep(step) {&#13;
	var table = getExecStepTable(step.sys_class_name);&#13;
	var grExcStep = new GlideRecord(table);&#13;
	grExcStep.initialize();&#13;
&#13;
	grExcStep.response_plan = current.sys_id.toString();&#13;
	grExcStep.response_plan_step = step.sys_id.toString();&#13;
	grExcStep.parent = current.sys_id.toString();&#13;
	&#13;
	if (table != CIRConstants.Tables.RESP_PLAN_EXEC_STEP_ACTN) {&#13;
		grExcStep.short_description = step.name.toString();&#13;
	} else {&#13;
		grExcStep.short_description = step.short_description.toString();&#13;
	}&#13;
	&#13;
	grExcStep.description = step.description.toString();&#13;
&#13;
	return grExcStep.insert();&#13;
&#13;
}&#13;
&#13;
function getExecStepTable(stepTable) {&#13;
	var tables = CIRConstants.Tables;&#13;
	switch (stepTable.toString()){&#13;
		case tables.RESP_PLAN_STEP_MSG:&#13;
			return tables.RESP_PLAN_EXEC_STEP_MSG;&#13;
&#13;
		case tables.RESP_PLAN_STEP_COND:&#13;
			return  tables.RESP_PLAN_EXEC_STEP_COND;&#13;
&#13;
		case tables.RESP_PLAN_STEP_ACTN:&#13;
			return  tables.RESP_PLAN_EXEC_STEP_ACTN;&#13;
&#13;
		default :&#13;
			return tables.RESP_PLAN_EXEC_STEP;&#13;
	}&#13;
}</value>
