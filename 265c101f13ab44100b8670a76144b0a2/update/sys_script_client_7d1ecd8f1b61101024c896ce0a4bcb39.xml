        <field>country</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Remove Country Popup</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	if (isLoading ) {
		return;
	}

	if(newValue.length < g_scratchpad.country.length && !g_form.isNewRecord()){              
		var w = new GlideModal("x_fru_cir_cir_situation_remove_country");
		w.setTitle(getMessage("cir_situation_remove_country_title"));
		w.setPreference("oldValue",  g_scratchpad.country);
		w.setPreference("newValue", newValue);
		w.render();
	}

	g_scratchpad.country = newValue;
}]]></script>
