        <script><![CDATA[new CIRPSIUtils().CreateFromRP(current, producer, 'x_fru_cir_internal_physical_security_incident');
// redirection logic

// sumbit and copy
if (producer.copied == 'true' && producer.state == '21') {
	var args = '&country=' + producer.country +
		'&state=' + producer.state_province +
		'&city=' + producer.city +
		'&building=' + producer.building_name +
		'&floor=' + producer.floor + '';

	if (producer.from_mobile == 'false') {
		producer.portal_redirect = 'cir?id=cir_psi_sc_cat_item&sys_id='+ RP.getParameterValue('sysparm_id') +'&category=' + producer.selected_cat + args;
	} else { 
		producer.portal_redirect = 'cir?id=cir_sc_cat_item_psi_mobile&sys_id='+ RP.getParameterValue('sysparm_id') +'&category='+ producer.selected_cat + args;
	}
	//  save as submited      
} else if (producer.copied == 'false' && producer.state == '21') {
	producer.portal_redirect = 'cir?id=cir_psi_sc_cat_item&sys_id='+ RP.getParameterValue('sysparm_id') + '&category=' + producer.selected_cat + '';
	// save as draft	
} else {
	if (producer.from_mobile == 'false'){
		producer.portal_redirect = "cir?id=cir_internal_psi_form&table=x_fru_cir_internal_physical_security_incident&sys_id=" + current.getUniqueValue();
	} else {
		producer.portal_redirect = '';
	}
}
current.setAbortAction(true);]]></script>
