        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	
	if (current.final_kilometers_dec + '' === '') {
		current.kilometers_reading = '';
	} else {
		if (current.final_kilometers_dec >= current.initial_kilometers) {
			current.kilometers_reading = current.final_kilometers_dec - current.initial_kilometers;
		} else {
			current.setAbortAction(true);
			gs.addErrorMessage(gs.getMessage('cir_kms_error'));
		} 
	}
	
})(current, previous);]]></script>
