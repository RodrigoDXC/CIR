        <name>Create Static Vendor Report</name>
        <onclick>createVendorReport()</onclick>
        <order>100</order>
        <script><![CDATA[function createVendorReport() { 
	var vReportDate = g_form.getValue('report_date');
	var vReportId = g_form.getUniqueValue();

	var dialog = new GlideDialogForm(getMessage('Vendor Report Details'), 'x_fru_cir_vendor_report_details'); 
	dialog.addParm('sysparm_view', 'default'); 
	dialog.setSysID('-1'); // -1 = new record;

	dialog.setLoadCallback(function(iframeDoc) {
		var dialogFrame = 'defaultView' in iframeDoc ? iframeDoc.defaultView : iframeDoc.parentWindow;
		dialogFrame.g_form.setValue('vendor_report', vReportId);
	});
	dialog.render(); 
}]]></script>
