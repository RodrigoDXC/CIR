        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var countryID = new CIRSituationUtils().GetCountryAddressID(current.country);
   
	var grOnCallT = new GlideRecord(CIRConstants.Tables.ON_CALL_TEAM);
	grOnCallT .addQuery('country','CONTAINS', countryID);
	grOnCallT .addQuery('dimension', current.dimension.toString());
	grOnCallT.query();
	
	if(grOnCallT.next()) {
		current.on_call_team = grOnCallT.getUniqueValue();
	}

})(current, previous);]]></script>
