<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sys_script_client">
    <sys_script_client action="INSERT_OR_UPDATE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description/>
        <field>date</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Populate agenda title from date</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	if (isLoading) {
		return;
	}

	if (!g_form.getValue('date') || !g_form.getValue('vip_alias')) {
		g_form.clearValue('agenda_title');
		return;
	}

	var date = g_form.getValue('date');
	var profile = g_form.getReference('vip_alias', callBack);
	var vipAlias = '';

	function callBack(profile) {
		if (profile) {
			vipAlias = profile.alias.toString();
			g_form.setValue('agenda_title', vipAlias + ' - ' + date);
		}
	}
}]]></script>
