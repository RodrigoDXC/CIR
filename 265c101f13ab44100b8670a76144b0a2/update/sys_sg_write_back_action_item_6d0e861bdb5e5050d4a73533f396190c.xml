    <sys_sg_write_back_action_item action="INSERT_OR_UPDATE">
        <description/>
        <execution_script><![CDATA[(function WriteBackAction(input) {

	var grExpenseLines = new GlideRecord(CIRConstants.Tables.EXPENSE_LINE);
	grExpenseLines.get(input.sys_id);

	grExpenseLines.corporate_car = input.corporate_card;
	grExpenseLines.value = input.value;
	grExpenseLines.update();
	
})(input);]]></execution_script>
