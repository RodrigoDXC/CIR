        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	if (isLoading){
		return;
	}

	var space = g_form.getValue('space');

	if (space) {
		g_form.setReadOnly('building_name', true);
	}

	if (!space) {
		g_form.setReadOnly('building_name', false);
		g_form.setValue('floor', '');
	}
}]]></script>
