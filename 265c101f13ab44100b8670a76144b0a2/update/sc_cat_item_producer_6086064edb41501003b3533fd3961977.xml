        <script><![CDATA[var defaultCaseGroup = gs.getProperty('x_fru_cir.default.case.manager.group');
var group = '';
var locationRec = new GlideRecord(CIRConstants.Tables.LOCATION);
locationRec.addQuery('sys_id', producer.location.toString());
locationRec.query();
if (locationRec.next()) {
	if (locationRec.x_fru_cir_case_management_group == '') {
		group = defaultCaseGroup;
	} else {
		group = locationRec.x_fru_cir_case_management_group;
	}
}
current.assignment_group = group;

if (producer.anonymous.toString() === 'Yes') {
    var caseObject = {
        className: producer.case_type.toString(),
        occurred_on: producer.occurred_on.toString(),
        location_type: producer.location_type.toString(),
        location: producer.location.toString(),
        location_notes: producer.location_notes.toString(),
        short_description: producer.short_description.toString(),
        description: producer.description.toString(),
        sysID: current.sys_id.toString(),
		group: group
    };
    gs.eventQueue('x_fru_cir.anonymous.case.submitted', null, new global.JSON().encode(caseObject), '');
	current.setAbortAction(true);
    if (!gs.isMobile()) {
        producer.portal_redirect = '?id=cir_ciri_homepage&info_message=anonymous_case_created';
    }

} else {
	current.sys_class_name = producer.case_type;
	current.reported_by = gs.getUserID();

    if (!gs.isMobile()) {
        producer.portal_redirect = '?id=cir_ciri_case&is_new_order=true&table=' + CIRConstants.Tables.CASE + '&sys_id=' + current
            .sys_id;
    }
}]]></script>
