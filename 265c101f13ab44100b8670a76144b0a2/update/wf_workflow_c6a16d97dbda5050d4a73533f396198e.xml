        <value>var utils = new CIRPersonalProtectionUtils();&#13;
&#13;
var grUserPref = new GlideRecord('sys_user_preference');&#13;
grUserPref.addQuery('user', current.bodyguard.user);&#13;
grUserPref.addQuery('name', 'user.language');&#13;
grUserPref.query();&#13;
&#13;
if (grUserPref.next()) {&#13;
	sendCorrectNotification(grUserPref.getValue('value'));&#13;
	&#13;
} else {&#13;
	sendCorrectNotification(gs.getProperty('glide.sys.language'));&#13;
}&#13;
&#13;
function sendCorrectNotification(language) {&#13;
	switch (language) {&#13;
		case 'es':&#13;
			utils.SendExpenseNotification('x_fru_cir.push.expense.rejected.es', current, current.bodyguard.user);&#13;
			break;&#13;
		default:&#13;
			utils.SendExpenseNotification('x_fru_cir.push.expense.rejected.en', current, current.bodyguard.user);&#13;
			break;&#13;
	}&#13;
}&#13;
