					var personSys = RP.getWindowProperties().personSys;
					var personImpact = new GlideRecordSecure('x_fru_cir_person_impact');
					if (personImpact.get(personSys)) {
						var caseDate = new GlideDateTime(personImpact.cir_case.occurred_on);
						var caseTime = caseDate.getLocalTime();
						var workStart = new GlideDateTime(personImpact.shift_start);
						workStart = workStart.getLocalTime();
						var personObject = {
							name: personImpact.first_name + ' ' + personImpact.last_name,
							dob: personImpact.date_of_birth.getDisplayValue().split('-'),
							startDate: personImpact.date_started_at_company.getDisplayValue().split('-'),
							gender: personImpact.gender,
							caseNumber: personImpact.cir_case.getDisplayValue(),
							caseDate: caseDate.getLocalDate().toString().split('-'),
							caseTime: caseTime.getByFormat('hh:mm:ss'),
							workStart: workStart.getByFormat('hh:mm:ss')
						};
					}
