        <insert_callback/>
        <name>Cases</name>
        <query_from/>
        <query_with><![CDATA[(function refineQuery(current, parent) {
    current.addQuery('number', 'IN', getCases());

    function getCases() {
        var cases = [];
        var grPhysicalImpact = new GlideRecord(CIRConstants.Tables.PHYSICAL_IMPACT);
        grPhysicalImpact.addQuery('affected_vehicle', parent.getUniqueValue());
        grPhysicalImpact.query();
        while (grPhysicalImpact.next()) {
            cases.push(grPhysicalImpact.getDisplayValue('cir_case'));
        }
        return cases;
    }
})(current, parent);]]></query_with>
