    var ga = new GlideAjax('CIRCaseManagementAjax');
    ga.addParam('sysparm_name', 'isCirRoleOnly');
    ga.getXMLAnswer(function(answer) {
        var parsedAnswer = JSON.parse(answer);
        if(parsedAnswer.isCirRoleOnly) {
            g_form.setDisabled('reported_by', true);
            g_form.setValue('reported_by', parsedAnswer.currentUserId, parsedAnswer.currentUserDisplayName);
        }
    });
}]]></script>
