        <script><![CDATA[function onLoad() {
	if (!g_form.getValue('country') && !g_form.getValue('inc_location')) {
		var ga = new GlideAjax('CIRPSIAjaxUtils');
		ga.addParam('sysparm_name', 'getUserCountryValue');
		ga.getXML(getData);
	}

	function getData(response) {
		var answer = response.responseXML.documentElement.getAttribute("answer");
		g_form.setValue('country', answer);
	}
}]]></script>
