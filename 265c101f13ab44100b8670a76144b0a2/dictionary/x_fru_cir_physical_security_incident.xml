        <element label="Last report" max_length="40" name="last_report" read_only="true" type="glide_date_time"/>
        <element attributes="edge_encryption_enabled=true,encode_utf8=false" label="Local currency" mandatory="true" max_length="32" name="local_currency" reference="fx_currency" type="reference"/>
        <element attributes="edge_encryption_enabled=true,no_sort=true,slushbucket_ref_no_expand=true" label="Report to" max_length="4000" name="report_to" reference="sys_user" type="glide_list"/>
        <element attributes="edge_encryption_enabled=true" choice="1" label="Result" mandatory="true" max_length="40" name="result" type="choice">
            <choice>
                <element inactive_on_update="false" label="Accomplished" sequence="1" value="1"/>
                <element inactive_on_update="false" label="Attempted" sequence="2" value="2"/>
                <element inactive_on_update="false" label="On-Going" sequence="3" value="3"/>
            </choice>
        </element>
        <element attributes="edge_encryption_enabled=true,encode_utf8=false" label="State/Province" max_length="32" name="state_province" reference="x_fru_cir_security_address_value" reference_qual="javascript:  var query = 'sys_idIN' + new CIRPSIUtils().AddrValueFilter('state',current);  + ''; query" type="reference" use_reference_qualifier="advanced"/>
        <element attributes="edge_encryption_enabled=true" label="Upload" max_length="40" name="upload" type="file_attachment"/>
        <element attributes="edge_encryption_enabled=true,encode_utf8=false" default="javascript: gs.getUserID();" label="User" mandatory="true" max_length="32" name="user" reference="sys_user" type="reference"/>
    </element>
</database>
