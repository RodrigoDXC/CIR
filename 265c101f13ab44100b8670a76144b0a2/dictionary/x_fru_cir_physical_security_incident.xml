<?xml version="1.0" encoding="UTF-8"?><database>
    <element extends="task" label="Physical Security Incident" max_length="40" name="x_fru_cir_physical_security_incident" sizeclass="7" type="collection">
        <element attributes="edge_encryption_enabled=true,encode_utf8=false" label="Asset type" mandatory="true" max_length="32" name="asset_type" reference="x_fru_cir_psi_asset_type" reference_qual="active=true^class=psi^EQ" reference_qual_condition="active=true^class=psi^EQ" type="reference"/>
        <element attributes="edge_encryption_enabled=true" label="Send notification after submitting?" max_length="40" name="auto_submit_notification" type="boolean"/>
        <element attributes="edge_encryption_enabled=true,encode_utf8=false,tree_picker=true" label="Category" mandatory="true" max_length="32" name="category" reference="x_fru_cir_psi_category" type="reference"/>
        <element attributes="edge_encryption_enabled=true,encode_utf8=false" label="City" max_length="32" name="city" reference="x_fru_cir_security_address_value" reference_qual="javascript:  var query = 'sys_idIN' + new CIRPSIUtils().AddrValueFilter('city',current);  + ''; query" type="reference" use_reference_qualifier="advanced"/>
        <element label="Converted cost" max_length="40" name="converted_cost" read_only="true" type="string"/>
        <element label="Cost of incident" max_length="40" name="cost_of_incident" type="string"/>
        <element attributes="edge_encryption_enabled=true" choice="1" choice_field="country" choice_table="cmn_location" label="Country" max_length="40" name="country" type="choice">
            <choice/>
        </element>
        <element default="javascript: var gdt = new GlideDateTime(); var date = gdt.getDate(); var gdt2 = new GlideDateTime(); gdt2.setDisplayValue(date + ' 12:00:00'); gdt2.getDisplayValue();" label="Date" mandatory="true" max_length="40" name="date_occurred" type="glide_date_time"/>
